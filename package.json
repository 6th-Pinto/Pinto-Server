{
  "name": "express-starter",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "start": "env-cmd node src/app.js",
    "start:dev": "env-cmd nodemon src/app.js",
    "build": "env-cmd -f node src/app.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier:write": "yarn prettier --write .",
    "prepare": "husky install",
    "precommit": "yarn lint-staged"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "path": "^0.12.7",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "env-cmd": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "lint-staged": "^12.3.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
